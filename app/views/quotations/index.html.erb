<div class="container">
  <div class="row">
    <h2><%= @owner.name %>様の見積もり一覧</h2>
  </div>

  <div class="row">
    <div>
      <%= link_to "見積もり追加", quotation_new_path(@owner), class: "btn btn-success" %>
    </div>
  </div>

  <div class="row">
    <div>
      <% @quotations.each do |quotation| %>
      <div>
        <%= link_to quotation_path(quotation) do %>
          <%= quotation.created_at %>
        <% end %>
      </div>
      <% end %>
    </div>
  </div>
</div>
